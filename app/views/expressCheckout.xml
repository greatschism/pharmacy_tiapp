<Alloy>
	
	<ScrollView id="scrollView">
		<View id="parentView" class="vgroup" visible="false">
			<ImageView id="logoImg" class="margin-top-large img-logo" />
			<Label id="titleLbl" class="margin-top margin-left-large margin-right-large h6" />
			<View class="margin-top-extra-large auto-height fill-width">
				<ImageView class="img-qr-bg width-80 auto-height fade-value" />
				<View class="margin-top-extra-large auto-height fill-width vgroup ">
					<Label id="dobLbl" class="margin-top margin-bottom margin-left margin-right bg-color txt-center h5" />
					<View class="margin-left margin-right auto-height bg-color vgroup border">
						<Widget id="dob" analyticsId="DobDropDwn" src="ti.dropdown" class="top left-disabled left dropdown border-disabled dob date" />
					</View>
					<Button id="generateCodeBtn" analyticsId="GenerateCodeBtn" class="primary-bg-color primary-light-fg-color primary-border h7" onClick="didClickGenerateCode"/>
				</View>
			</View>
		</View>
	</ScrollView>
</Alloy>