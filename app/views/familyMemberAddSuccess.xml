<Alloy>
	<ScrollView class="vgroup">
		<View class="auto-height">
			<ImageView id="addSuccessImg" class="margin-top-large"/>
			<Label id="addLbl" class="h4 margin-left margin-right h4 txt-center"/>
		</View>
		<Label id="addMsg" class="margin-top margin-left margin-right h4 txt-center"/>
		<Button id="addBtn" class="top-disabled bg-color margin-top primary-fg-color primary-border" onClick="didAddAnotherMember"/>
		<Button id="doneBtn" class="bg-color primary-fg-color primary-border" onClick="didClickDone"/>
	</ScrollView>
</Alloy>