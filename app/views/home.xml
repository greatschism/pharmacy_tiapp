<Alloy>
	<Widget id="rightNavBtn" src="ti.navbutton" class="nav-btn" onClick="didClickRightNav" if="!Alloy.Globals.isLoggedIn" />
	<Widget id="feedbackDialog" src="ti.modaldialog" class="margin-left margin-right">
		<ScrollView class="auto-height vgroup">
			<Label id="titleLbl" class="margin-top title-centered-wrap" />
			<Label id="descLbl" class="margin-top lbl-centered-wrap" />
			<Widget id="feedbackTxta" src="ti.textarea" class="margin-top txta feedback returnkey-done" onReturn="didClickSubmit"  />
			<Button id="submitBtn" class="margin-top primary-btn" onClick="didClickSubmit" />
			<Button id="cancelBtn" class="margin-top margin-bottom tertiary-btn" onClick="didClickCancel" />
		</ScrollView>
	</Widget>
	<View id="contentView" />
</Alloy> 