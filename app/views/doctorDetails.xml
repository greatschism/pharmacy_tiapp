<Alloy>
	<Widget id="photoDialog" src="com.mscripts.optiondialog" class="h4" onClick="didClickOption"  />
	<Button id="editBtn" class="role-right-nav-btn nav-icon-btn" onClick="didClickEdit" />
	<!--<View>-->
	<TableView id="tableView" height="Ti.UI.FILL" top="0" onClick="didItemClick">
		<HeaderView>
			<View class="auto-height vgroup ">

				<View id="profileNameView" class="auto-height paddingTop paddingBottom paddingLeft">
					<Label id="nameLbl" class="left multi-line fill-width s1" />
				</View>
				<View class="auto-height top">  

					<View id="profileImageView" class="padding-left auto-height auto-width top" >
						<Label id="profileIconLabel" height="90" width="90" class="left" onClick="didClickProfileImg" />
					</View>
					<View class="auto-height top detailsWidth hgroup right" >
						<View class=" paddingLeft paddingTop auto-height" onClick="phoneDialer">
							<Label id="phoneIconLbl" class="top left small-icon" />
							<Label id="phoneLbl" class="after-icon s3 multi-line " />
						</View>
						<View class="hseparator paddingLeft paddingTop"/>
						<View class="paddingTop paddingLeft auto-height">
							<Label id="faxIconLbl" class="top left small-icon" />
							<Label id="faxLbl" class="after-icon s3 multi-line " />
						</View>
						<View class="hseparator paddingLeft paddingTop"/>
						<View class="paddingTop paddingLeft auto-height">
							<Label id="directionIconLbl" class="top left small-icon"  />
							<Label id="directionLbl" class="after-icon s13 " onClick="didClickDirections" />
						</View>
					</View>
				</View>
				<Widget id="notesTxta" src="com.mscripts.textarea" class="txta paddingTop paddingBottom"/>
			</View>
		</HeaderView>
	</TableView>
	<!--</View>
	<View id="footerView" class="bottom">
	<Button id="hideDoctorBtn" class="btn-primary" />
	</View>
	</View>-->
</Alloy>