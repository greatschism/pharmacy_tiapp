<Alloy>
	<Widget id="photoDialog" src="com.mscripts.optiondialog" class="h4" onClick="didClickOption"  />
	<Button id="editBtn" class="nav-right-btn" onClick="didClickEdit" />
	<!--<View>-->
	<TableView id="tableView" height="Ti.UI.FILL" top="0" >
		<HeaderView>
			<View class="auto-height vgroup ">
				<View class="auto-height " >
					<View id="profileImageView" class="left top auto-height auto-width">
					<Label id="profileIconLabel" height="70" width="70" classes="paddingTop paddingLeft" />
					<Label id="changeImgLbl" class="padding-left multi-line" top="80" onClick="didClickProfileImg" />
					</View>	
					<View class="auto-height top width-75 hgroup right" >
						
						<View class="paddingLeft auto-height paddingTop hgroup" >
						<Label id="nameLbl" class="left h1 multi-line " />
						<Label id="editLbl" class="paddingLeft h14  multi-line" text="Edit" onClick="didClickEdit"/>
						</View>
						
						<View class="hseparator paddingLeft paddingTop"/>
						
						<View class=" paddingLeft paddingTop auto-height" onClick="phoneDialer">
							<Label id="phoneIconLbl" class="top left small-icon" />
							<Label id="phoneLbl" class="after-icon h15 multi-line fg-secondary" />
						</View>
						
						<View class="hseparator paddingLeft paddingTop"/>
						
						<View class="paddingTop paddingLeft auto-height">
							<Label id="faxIconLbl" class="top left small-icon" />
							<Label id="faxLbl" class="after-icon h15NoColor multi-line fg-secondary" />
						</View>
						
						<View class="hseparator paddingLeft paddingTop"/>
						
						<View class="paddingTop paddingLeft auto-height">
							<Label id="directionIconLbl" class="top left small-icon"  />
							<Label id="directionLbl" class="after-icon h15  fg-secondary" onClick="didClickDirections" />
						</View>
						
					</View>
				</View>
				<Widget id="notesTxta" src="com.mscripts.textarea" class="txta paddingTop paddingBottom"/>
			</View>
		</HeaderView>
	</TableView>
	<!--</View>
	<View id="footerView" class="bottom">
	<Button id="hideDoctorBtn" class="btn-primary" />
	</View>
	</View>-->
</Alloy>