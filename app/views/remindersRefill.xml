<Alloy>
	<View id="headerView" class="top auto-height vgroup">
		<Require id="patientSwitcher" src="templates/patientSwitcher" onChange="getAllPrescriptions" />
		<View class="content-header-view">
			<Label id="benefitsLbl" class="content-header-lbl-with-ricon" />
			<Button class="content-header-right-icon icon-help" onClick="didClickBenefits" />
		</View>
	</View>
	<TableView id="tableView" onClick="didClickTableView">
		<FooterView platform="ios">
			<View class="auto-height" />
		</FooterView>
	</TableView>
</Alloy>