<Alloy autoStyle="true">
	<!--swipeable-->
	<TableViewRow id="row" class="row-selection-disabled" if="$.args.hasChild">
		<View id="containerView" class="container-view" onTouchstart="didTouchstart" onTouchmove="didTouchmove" onTouchend="didTouchend" onTouchcancel="didTouchend">
			<View id="colorView" class="content-left-color-box" />
			<View id="contentView" class="content-view-vgroup-with-lcolor-box-child">
				<Label id="titleLbl" />
				<Label id="subtitleLbl" />
			</View>
			<Label class="content-child-icon icon-thin-arrow-right" />
			<View id="swipeView" class="swipe-view" />
		</View>
	</TableViewRow>
	<TableViewRow id="row" class="row-selection-disabled" if="!$.args.hasChild">
		<View id="containerView" class="container-view" onTouchstart="didTouchstart" onTouchmove="didTouchmove" onTouchend="didTouchend" onTouchcancel="didTouchend">
			<View id="colorView" class="content-left-color-box" />
			<View id="contentView" class="content-view-vgroup-with-lcolor-box">
				<Label id="titleLbl" />
				<Label id="subtitleLbl" />
			</View>
			<View id="swipeView" class="swipe-view" />
		</View>
	</TableViewRow>
</Alloy>