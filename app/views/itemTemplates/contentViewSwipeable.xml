<Alloy autoStyle="true">
	<TableViewRow id="row" class="row-selection-disabled">
		<View id="containerView" class="container-view" onTouchstart="didTouchstart" onTouchmove="didTouchmove" onTouchend="didTouchend" onTouchcancel="didTouchend">
			<View class="content-left-image-bordered">
				<Widget id="img" src="ti.remoteimage" class="width-100" onError="didError" />
			</View>
			<View id="contentView" class="content-view-vgroup-with-limage-child">
				<Label id="titleLbl" />
				<Label id="subtitleLbl" />
			</View>
			<Label class="content-child-icon icon-thin-arrow-right" />
			<View id="swipeView" class="swipe-view" />
		</View>
	</TableViewRow>
</Alloy>