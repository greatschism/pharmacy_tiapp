<Alloy>
	<TableViewRow id="row" class="row-selection-disabled">
		<View id="containerView" class="auto-height" onTouchstart="didTouchstart" onTouchmove="didTouchmove" onTouchend="didTouchend" onTouchcancel="didTouchend">
			<View id="contentView" class="margin-top margin-bottom margin-left margin-right auto-height">
				<View id="masterView" class="left width-65 auto-height vgroup">
					<Label id="titleLbl" class="left h4" />
					<Label id="subtitleLbl" class="margin-top-small left inactive-fg-color" />
				</View>
				<View id="detailView" class="right width-35 auto-height vgroup">
					<Label id="detailTitleLbl" class="right txt-right" />
					<Label id="detailSubtitleLbl" class="margin-top-small right txt-right" />
				</View>
			</View>
			<View id="swipeView" class="auto-height" />
		</View>
	</TableViewRow>
</Alloy>