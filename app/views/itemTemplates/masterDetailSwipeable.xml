<Alloy>
	<TableViewRow id="row" class="row-selected-bg-color-disabled">
		<View id="containerView" class="auto-height">
			<View id="swipeView" class="right bg-color auto-height bubble-disabled" />
			<View id="contentView" class="right width-100 bg-color">
				<View id="marginView" class="margin-top margin-bottom margin-left margin-right auto-height">
					<View id="masterView" class="left width-65 auto-height vgroup">
						<Label id="titleLbl" class="left h4 wrap-disabled" />
						<Label id="subtitleLbl" class="margin-top-small left inactive-fg-color wrap-disabled" />
					</View>
					<View id="detailView" class="right width-35 auto-height vgroup">
						<Label id="detailTitleLbl" class="right txt-right wrap-disabled" />
						<Label id="detailSubtitleLbl" class="margin-top-small right txt-right wrap-disabled" />
					</View>
				</View>
			</View>
			<View id="dragView" class="right width-100" onTouchstart="didTouchstart" onTouchmove="didTouchmove" onTouchend="didTouchend" onTouchcancel="didTouchend" />
		</View>
	</TableViewRow>
</Alloy>