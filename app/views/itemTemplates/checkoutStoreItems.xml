<Alloy autoStyle="true">
	<TableViewRow id="row">
		<View id="contentView" class="margin-top margin-bottom margin-left margin-right auto-height"  onClick="didClickItem" >
			<View class="auto-height vgroup">
				<Label id="titleLbl" class="margin-left h5 auto-height bubble-disabled" onClick="didClickPhone" />
				<Label id="subtitleLbl" class="margin-top-small margin-left inactive-fg-color auto-height" />
				<View id="amountDueView" class="top margin-left margin-right auto-height hgroup">
					<Label id="amountDueLbl" class="top left inactive-fg-color" />
					<Label id="amountDueVal" class="top margin-left-extra-large positive-fg-color h5" />
				</View>
			</View>
		</View>
	</TableViewRow>
</Alloy>