<Alloy>
	<TableViewRow id="row" class="row-selection-disabled">
		<View id="contentView" class="margin-top margin-bottom margin-left margin-right auto-height">
			<View id="masterView" class="left width-50 auto-height vgroup">
				<Label id="titleLbl" class="left h4 wrap-disabled" />
				<Label id="subtitleLbl" class="margin-top-small left inactive-fg-color wrap-disabled" />
			</View>
			<Button id="detailBtn" class="top-disabled left-disabled right width-50 bg-color fg-primary-color primary-border bubble-disabled" onClick="didClickDetail" />
		</View>
	</TableViewRow>
</Alloy>