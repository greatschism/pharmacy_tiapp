<Alloy>
	<View class="vgroup">
		<View class="content-header-view-wrap vgroup ">
			<Label id="transferRxTypeTitle" class="lbl-wrap margin-top margin-bottom"/>
		</View>
		<View class="margin-top form-vgroup">
			<Widget id="prescNameTxt" src="ti.textfield" class="form-txt returnkey-next" onReturn="moveToNext"  />
			<View class="h-divider"></View>
			<Widget id="prescNumberTxt" src="ti.textfield" class="form-txt returnkey-next" onReturn="moveToNext"  />
		</View>
		<View class="content-header-view vgroup margin-top margin-bottom">
			<Label id="pharmacyFromTitle" class="lbl-wrap margin-top"/>
		</View>
		<View class="margin-top form-vgroup">
			<Widget id="phoneNumberTxt" src="ti.textfield" class="form-txt returnkey-next" onReturn="moveToNext" onChange="didChange" />
		</View>
		<Button id="btnNext" class="primary-btn margin-top" onClick="didClickNext"/>
	</View>
</Alloy>
