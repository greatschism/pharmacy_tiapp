<Alloy>
	<TableView dataCollection="termsAndConditions" onClick="didClickItem">
		<HeaderView>
			<View class="section-header-view">
				<Label id="titleLbl" class="section-header-lbl" />
			</View>
		</HeaderView>
		<TableViewRow itemId="{id}">
			<View class="list-item-view-with-child">
				<Label class="left list-item-lbl" text="{agreement_name}" />
			</View>
			<Label class="list-item-child" />
		</TableViewRow>
		<FooterView>
			<View class="auto-height">
				<Button id="doneBtn" class="btn-padding-top margin-bottom primary-btn" onClick="didClickDone" />
			</View>
		</FooterView>
	</TableView>
</Alloy>