<Alloy>
	<TableView dataCollection="termsAndConditions" onClick="didItemClick">
		<TableViewRow itemUrl="{url}">
			<Label text="{name}" class="list-item-title" />
			<Label class="list-item-child" />
		</TableViewRow>
		<FooterView>
			<View class="auto-height">
				<Button id="doneBtn" class="margin-bottom btn-primary" onClick="didDoneClick" />
			</View>
		</FooterView>
	</TableView>
</Alloy>