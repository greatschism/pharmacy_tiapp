<Alloy>
	
	<TableView dataCollection="termsAndConditions" onClick="didItemClick">
		<HeaderView>
			<View class="auto-height">
				<Label id="docLabel" class="padding-left"  />
				<View class="hseparator" />
			</View>
		</HeaderView>
		<TableViewRow itemUrl="{url}">
			<Label text="{name}" class="list-item-title-lbl padding-left" />
			<Label class="list_item_child childIcon" />
		</TableViewRow>
		<FooterView>
			<View class="auto-height padding-top">
				<Button id="doneBtn" class="margin-bottom primary-btn padding-top" onClick="didDoneClick" />
			</View>
		</FooterView>
	</TableView>
</Alloy>