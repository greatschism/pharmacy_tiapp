<Alloy>
	<TableView class="bg-primary" dataCollection="menuItems" dataTransform="transformFunction" onClick="didItemClick">
		<HeaderView>
			<View class="vspacer" />
		</HeaderView>
		<TableViewRow class="height-default bg-primary">
			<View class="padding-left padding-left hgroup no-hwrap">
				<Label text="{icon}" class="icon color-primary" />
				<Label text="{title}" class="padding-left h3 color-primary" />
			</View>
		</TableViewRow>
	</TableView>
</Alloy>