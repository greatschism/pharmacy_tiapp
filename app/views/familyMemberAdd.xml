<Alloy>
	<ScrollView class="width-100 vgroup" height="115%" disableBounce="true">
		<View id="parentView" class="width-100 vgroup height-100 top-10">
			<Label id="familyMemberAddLbl" class="margin-left margin-right h2 txt-center"/>
			<ImageView id="imgFamilyAdd" class="margin-top-large"/>
			<Label id="familyMemberAddTip" class="margin-top-large h4 margin-left margin-right"/>
			<View class="margin-top-large margin-left margin-right auto-height vgroup border">
				<Widget id="dobDp" analyticsId="DobDropDwn" src="ti.dropdown" class="top left-disabled left dropdown border-disabled dob date" />
			</View>
			<Label id="familyMemberRelationTip" class="margin-top margin-left h4 margin-right txt-left"/>
			<View class="margin-top-large margin-left margin-right auto-height vgroup border">
				<Widget id="relationshipDp" analyticsId="RelationshipDropDwn" src="ti.dropdown" class="top left-disabled left dropdown border-disabled" onReturn="didChangeRelationship"/>
			</View>
			<View id="otherTxtView" class="margin-top margin-left margin-right auto-height vgroup border" ></View>
			<Button id="continueBtn" analyticsId="ContinueBtn" class="margin-bottom primary-bg-color primary-light-fg-color primary-border" onClick="didClickContinue"/>
		</View>
	</ScrollView>
</Alloy>