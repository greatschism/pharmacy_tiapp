	width: 20
},
".vspacer":{
	height: 20
},
".hseparator":{
	height: 1,
	backgroundColor: "#8b8b8b"
},
".vseparator":{
	width: 1,
	backgroundColor: "#8b8b8b"
},
/**
* text alignment
*/
".text-left":{
	textAlign: "left"
},
".text-center":{
	textAlign: "center"
},
".text-right":{
	textAlign: "right"
},
/**
* enable multi line
*/
".multi-line[platform=android,mobileweb]":{
	ellipsize: false,
	wordWrap: true
},
