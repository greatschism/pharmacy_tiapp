/*
*navbar button
*/
".nav-bar-right-view":{
	right: 0,
	width: 60,
	height: 40,
	role: "navBar"
},
".nav-bar-right-btn":{
	right: 0,
	width: 60,
	height: 25,
	color: "#fff",
	backgroundColor: "transparent",
	borderColor: "transparent",
	role: "navBar"
},
".nav-bar-right-btn[platform=ios]":{
	font:{
		fontFamily: Alloy.CFG.fonts.regular,
		fontSize: 17
	}
},
".nav-bar-right-btn[platform=android,mobileweb]":{
	font:{
		fontFamily: Alloy.CFG.fonts.light,
		fontSize: 17
	}
},
/*
*button with image on left / right
*/
".button-view":{
