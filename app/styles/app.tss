"Window":{
	backgroundColor: "#fff"
},
"Window[platform=ios]":{
	navTintColor: "#fff",
	statusBarStyle: Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT
},
"ListView":{
	separatorColor: "#ccc"
},
"ListView[platform=ios]":{
	separatorInsets: {
		top: 0,
		left: 0
	}
},
"ListItem[platform=ios]":{
	selectionStyle: Ti.UI.iPhone.ListViewCellSelectionStyle.NONE
},
"TableView":{
	separatorColor: "#ccc"
},
"TableViewRow[platform=ios]":{
	selectionStyle: Ti.UI.iPhone.TableViewCellSelectionStyle.NONE,
	separatorInsets: {
		top: 0,
		left: 0
	}
},
"Label":{
	font:{
		fontFamily: Alloy.CFG.fonts.regular,
		fontSize: 13
	},
	textAlign: "left"
},
"Label[platform=android,mobileweb]":{
	ellipsize: true,
	wordWrap: false
},
"Button":{
	width: "90%",
	height: 50,
	font:{
		fontFamily: Alloy.CFG.fonts.regular
	},
	borderRadius: 5
},
"ImageView[platform=ios]":{
	preventDefaultImage: true
},
/**************************************************************************
* General Helper and Utility Styles
*************************************************************************/
".modal":{
	modal: true
},
".touch-disabled":{
	touchEnabled: false
},
".rounded-corner-4":{
	borderRadius: 4,
	borderWidth: 1
},
/**
* 	Visibility
*/
".hide":{
	visible: false
},
".show":{
	visible: true
},
/**
* 	Layouts & Fills
*/
".vgroup":{
	layout: "vertical"
},
".hgroup":{
	layout: "horizontal"
},
".no-hwrap":{
	horizontalWrap: false
},
".fill":{
	width: Ti.UI.FILL,
	height: Ti.UI.FILL
},
".fill-width":{
	width: Ti.UI.FILL
},
".fill-height":{
	height: Ti.UI.FILL
},
".auto":{
	width: Ti.UI.SIZE,
	height: Ti.UI.SIZE
},
".auto-width":{
	width: Ti.UI.SIZE
},
".auto-height":{
	height: Ti.UI.SIZE
},
".top":{
	top: 0
},
".bottom":{
	bottom: 0
},
".left":{
	left: 0
},
".right":{
	right: 0
},
".top-5":{
	top: 5
},
".bottom-5":{
	bottom: 5
},
".left-5":{
	left: 5
},
".right-5":{
	left: 5
},
".top-10":{
	top: 10
},
".top-20":{
