"#titleLbl":{
	text: Alloy.Globals.strings.loginLblTitle
},
"#unameTxt":{
	hintText: Alloy.Globals.strings.loginHintUsername,
	nextItem: "passwordTxt"
},
"#passwordTxt":{
	hintText: Alloy.Globals.strings.hintPassword,
	passwordMask: true
},
"#keepMeLbl":{
	right: 60,
	text: Alloy.Globals.strings.loginLblRememberMe
},
"#loginBtn":{
	title: Alloy.Globals.strings.loginBtnLogin
},
"#createAccountLbl":{
	html: Alloy.Globals.strings.loginLnkCreateAccount
},
"#keepMeSwt[platform=android]":{
	accessibilityLabelOn: Alloy.Globals.strings.accessibilityLblSwitchOn,
	accessibilityLabelOff: Alloy.Globals.strings.accessibilityLblSwitchOff
}