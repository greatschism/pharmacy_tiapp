"#usernameTxt":{
	hintText: Alloy.Globals.strings.loginHintUsername,
	keyboardType: Titanium.UI.KEYBOARD_EMAIL,
	nextItem: "passwordTxt"
},
"#passwordTxt":{
	hintText: Alloy.Globals.strings.loginHintPassword,
	passwordMask: true
},
"#autoLoginLbl":{
	text: Alloy.Globals.strings.loginLblAutoLogin
},
"#loginBtn":{
	title: Alloy.Globals.strings.loginBtnLogin
},
"#signupAttr":{
	html: Alloy.Globals.strings.loginAttrSignup
},
"#forgotPwdAttr":{
	html: Alloy.Globals.strings.loginAttrForgotPassword
},
"#tooltipLbl":{
	text:Alloy.Globals.strings.loginTooltipLblAutoSignIn
},
"#tooltipHideBtn":{
	title:Alloy.Globals.strings.loginTooltipBtnHide
},
"#aboutAttr":{
	text: Alloy.Globals.strings.loginAttrAbout
}