	nextItem: "fnameTxt"
},
"#fnameTxt":{
	hintText:Alloy.Globals.strings.mgrAccountCreationHintFirstName,
	nextItem: "lnameTxt"
},
"#lnameTxt":{
	hintText:Alloy.Globals.strings.mgrAccountCreationHintLastName,
	nextItem: "dobDp"
},
"#dobDp":{
	hintText:Alloy.Globals.strings.mgrAccountCreationHintDob,
	nextItem: "emailTxt"
},
"#emailTxt":{
