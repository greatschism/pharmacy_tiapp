},
"#mgrTitleLbl":{
	text:Alloy.Globals.strings.mgrAccountCreationTitle
},
"#mgrSubTitleLbl":{
	text:Alloy.Globals.strings.mgrAccountCreationSubTitle
},
"#continueBtn":{
	title:Alloy.Globals.strings.mgrAccountCreationBtnContinue
},
"#agreementAttr":{
	html:Alloy.Globals.strings.mgrAccountCreationAttrAgreement
