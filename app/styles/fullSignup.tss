"#logoImg":{
	width: 75,
	image: "/images/login/pharmacy.png"
},
"#hintLbl":{
	text: Alloy.Globals.Strings.lblSignupAndGetRefillInSeconds
},
".vseparator":{
	zIndex: 1
},
".txt":{
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},
"#fnameTxt":{
	hintText: Alloy.Globals.Strings.hintFname,
	nextItem: "lnameTxt"
},
"#lnameTxt":{
	hintText: Alloy.Globals.Strings.hintLname,
	nextItem: "emailTxt"
},
"#dob":{
	hintText: Alloy.Globals.Strings.hintDOB,
	type: Ti.UI.PICKER_TYPE_DATE,
	rightImage: "/images/down_angle_gray.png"
},
"#inputView":{
	zIndex: 1
},
"#passwordTooltip":{
	top: 50,
	text: Alloy.Globals.Strings.msgPasswordTips,
	zIndex: 2
},
"#emailTxt":{
	hintText: Alloy.Globals.Strings.hintEmail,
	keyboardType: Titanium.UI.KEYBOARD_EMAIL,
	nextItem: "unameTxt"
},
"#unameTxt":{
	hintText: Alloy.Globals.Strings.hintUsername,
	nextItem: "passwordTxt"
},
"#passwordTxt":{
	right: 75,
	hintText: Alloy.Globals.Strings.hintPassword,
	nextItem: "rxNoTxt",
	passwordMask: true
},
"#toggleSwt":{
	enabledColor: "#F79538",
	value: true
},
"#rxNoTxt":{
	hintText: Alloy.Globals.Strings.hintRxNo,
	returnKeyType: Ti.UI.RETURNKEY_DONE
},
"#locationLbl":{
	left: 8,
	right: 40,
	text: Alloy.Globals.Strings.hintPharmacyLocation
},
"#downImg":{
	image: "/images/down_angle_gray.png"
},
"#agreementLbl":{
	html: Alloy.Globals.Strings.lblSignupAgreement
},
"#singupLbl":{
	text: Alloy.Globals.Strings.strSignup
},
"#singupImg":{
	width: 20,
	image: "/images/right_arrow_orange.png"
}
