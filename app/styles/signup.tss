"#checkImg":{
	width: 75,
	image: "/images/checked.png"
},
"#hintLbl":{
	text: Alloy.Globals.strings.lblGreatYourPhoneNumberIsConfirmed
},
".txt":{
	returnKeyType: Ti.UI.RETURNKEY_NEXT
},
"#inputView":{
	zIndex: 1
},
"#passwordTooltip":{
	text: Alloy.Globals.strings.msgPasswordTips,
	zIndex: 2
},
"#passwordTooltip[platform=ios,android]":{
	top: 50
},
"#passwordTooltip[platform=mobileweb]":{
	top: 105
},
"#unameTxt":{
	hintText: Alloy.Globals.strings.hintUsername,
	nextItem: "emailTxt"
},
"#emailTxt":{
	hintText: Alloy.Globals.strings.hintEmail,
	keyboardType: Titanium.UI.KEYBOARD_EMAIL,
	nextItem: "passwordTxt"
},
"#passwordTxt":{
	right: 75,
	hintText: Alloy.Globals.strings.hintPassword,
	passwordMask: true,
	returnKeyType: Ti.UI.RETURNKEY_DONE
},
"#toggleSwt":{
	value: true
},
"#agreementLbl":{
	html: Alloy.Globals.strings.lblSignupAgreement
},
"#createBtn":{
	title: Alloy.Globals.strings.btnCreateAccount
}
