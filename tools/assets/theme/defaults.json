{
	"Theme": {
		"version": "Alloy.TSS.Theme.version"
	},
	"Window[platform=ios]": {
		"statusBarStyle": "Ti.UI.iPhone.StatusBar.DEFAULT"
	},
	"Window[platform=android]": {
		"windowSoftInputMode": "Ti.UI.Android.SOFT_INPUT_STATE_HIDDEN | Ti.UI.Android.SOFT_INPUT_ADJUST_RESIZE"
	},
	"Window[formFactor=handheld]": {
		"orientationModes": ["Ti.UI.PORTRAIT"]
	},
	"Window[formFactor=tablet]": {
		"orientationModes": ["Ti.UI.PORTRAIT", "Ti.UI.UPSIDE_PORTRAIT"]
	},
	"TableView": {
		"footerDividersEnabled": true,
		"filterAttribute": "Alloy.Globals.filterAttribute"
	},
	"ListView": {
		"footerDividersEnabled": true
	},
	"TableViewRow": {
		"height": "Ti.UI.SIZE"
	},
	"ListItem": {
		"height": "Ti.UI.SIZE"
	},
	"Button[platform=ios]": {
		"style": "Ti.UI.iPhone.SystemButtonStyle.PLAIN"
	},
	"ImageView": {
		"width": "Ti.UI.SIZE",
		"height": "Ti.UI.SIZE"
	},
	"ImageView[platform=ios]": {
		"preventDefaultImage": true
	},
	"Label": {
		"wordWrap": true,
		"ellipsize": false,
		"height": "Ti.UI.SIZE",
		"textAlign": "Ti.UI.TEXT_ALIGNMENT_LEFT"
	},
	"Button": {
		"textAlign": "Ti.UI.TEXT_ALIGNMENT_CENTER",
		"backgroundColor": "transparent"
	},
	".drawer": {
		"leftDrawerWidth": "Alloy.CFG.left_drawer_width"
	},
	".drawer[formFactor=handheld]": {
		"orientationModes": ["Ti.UI.PORTRAIT"]
	},
	".drawer[formFactor=tablet]": {
		"orientationModes": ["Ti.UI.PORTRAIT", "Ti.UI.UPSIDE_PORTRAIT"]
	},
	".rx": {
		"maxLength": "Alloy.CFG.rx_length"
	},
	".date": {
		"title": "Alloy.Globals.strings.dialogTitleDatePicker",
		"leftTitle": "Alloy.Globals.strings.dialogBtnCancel",
		"rightTitle": "Alloy.Globals.strings.dialogBtnOK",
		"format": "Alloy.CFG.date_format",
		"type": "Ti.UI.PICKER_TYPE_DATE"
	},
	".time": {
		"title": "Alloy.Globals.strings.dialogTitleTimePicker",
		"leftTitle": "Alloy.Globals.strings.dialogBtnCancel",
		"rightTitle": "Alloy.Globals.strings.dialogBtnOK",
		"format": "Alloy.CFG.time_format",
		"type": "Ti.UI.PICKER_TYPE_TIME"
	},
	".dob": {
		"selectedDate": "Alloy.CFG.dob_default_date"
	},
	".time-reminder": {
