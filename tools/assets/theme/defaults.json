{
	"Theme": {
		"version": "Alloy.TSS.Theme.version"
	},
	"Window[platform=ios]": {
		"statusBarStyle": "Ti.UI.iOS.StatusBar.DEFAULT"
	},
	"Window[platform=android]": {
		"windowSoftInputMode": "Ti.UI.Android.SOFT_INPUT_STATE_HIDDEN | Ti.UI.Android.SOFT_INPUT_ADJUST_RESIZE"
	},
	"Window[formFactor=handheld]": {
		"orientationModes": ["Ti.UI.PORTRAIT"]
	},
	"Window[formFactor=tablet]": {
		"orientationModes": ["Ti.UI.PORTRAIT", "Ti.UI.UPSIDE_PORTRAIT"]
